# Environment variables - DO NOT COMMIT THIS FILE!
# Copy to .env and fill in your actual API keys

# Primary APIs (main translation workhorses)
GEMINI_PRIMARY_API1_KEY=your_primary_api_key_1_here
GEMINI_PRIMARY_API2_KEY=your_primary_api_key_2_here

# Fallback APIs (used when primary fails)
GEMINI_FALLBACK_API1_KEY=your_fallback_api_key_3_here
GEMINI_FALLBACK_API2_KEY=your_fallback_api_key_4_here

# API Endpoint template
# {model} will be replaced with selected model (gemini-2.5-flash, etc.)
GEMINI_API_ENDPOINT=https://generativelanguage.googleapis.com/v1beta/models/{model}:generateContent

# Optional: Set default model
DEFAULT_GEMINI_MODEL=gemini-2.5-flash

# Optional: Rate limiting (requests per second per API)
RATE_LIMIT_PER_API=5
